TOP = ../..
SUBDIRS = test-pam test-server test-ipc

export DOINSTALL=yes

include $(TOP)/GNUmakefile.global

# all::
# 	gmake pam
# 	gmake server
# 	gmake ipc
# 
# pam::
# 	gmake -f GNUmakefile.pam
# 
# server::
# 	gmake -f GNUmakefile.server
# 
# ipc::
# 	gmake -f GNUmakefile.ipc
# 
# install::
# 	gmake -f GNUmakefile.pam install
# 	gmake -f GNUmakefile.server install
# 	gmake -f GNUmakefile.ipc install
